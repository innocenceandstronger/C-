#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<assert.h>
#include<windows.h>
char* strcpy(char *dept, const char *src)
{
	char *ret = dept;
	assert(dept != NULL);
	assert(src != NULL);
	while (*dept++ = *src++)
	{
		;
	}
	return ret;
}
int main()
{
	char arr1[] = " abcdef ";
	char arr2[20] = { 0 };
	printf("%s\n", strcpy(arr2,arr1));
	system("pause");
	return 0;
}
